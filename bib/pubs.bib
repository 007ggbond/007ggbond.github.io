	file = {ScienceDirect Full Text PDF:C\:\\Users\\xiaoh\\OneDrive\\Documents\\Zotero\\storage\\9D4U7KT6\\Fang 等。 - 2018 - Investigation of inner mechanism of anisotropic me.pdf:application/pdf}
}

